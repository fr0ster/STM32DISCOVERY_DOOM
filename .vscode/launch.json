    "version": "0.2.0",
    "configurations": [
        {
            "name": "OpenOCD Debug",
            "request": "launch",
            "type": "cortex-debug",
            "cwd": "${workspaceRoot}",
            "servertype": "openocd",
            "executable": "./build/STM32DISCOVERY_DOOM.elf",
            "svdFile": "${workspaceRoot}/scripts/STM32F429.svd",
            "configFiles": [
                "${workspaceRoot}/scripts/openocd-jlink-swd.cfg"
            ]
        },
        {
            "name": "JLink Debug",
            "cwd": "${workspaceRoot}",
            "executable": "./build/STM32DISCOVERY_DOOM.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "svdFile": "${workspaceRoot}/scripts/STM32F429.svd",
            "device": "STM32F429ZI",
            "interface": "swd"
        }
    ]
}
